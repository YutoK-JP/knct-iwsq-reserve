{
  "extends": [
    "@kobakazu0429/eslint-config-typescript-react",
    "next/core-web-vitals"
  ],
  "plugins": ["import-access"],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "import-access/jsdoc": ["error", {
      "indexLoophole": true,
      "filenameLoophole": false,
      "defaultImportability": "package", // "public" | "package" | "private"
      "treatSelfReferenceAs": "external" // "internal" | "external"
    }]
  }
}
